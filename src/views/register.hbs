<div class="checklogin">
  <h3>{{checkresgister}}</h3>
</div>
<form method="POST" action="/register">
  <div class="form-group">
    <label for="exampleInputEmail1">Email address</label>
    <input type="email" class="form-control" id="email" name="email">
    <small id="emailHelp" class="form-text text-muted">We'll never share your email with anyone else.</small>
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Password</label>
    <input type="password" class="form-control" id="password" placeholder="Password" name="password">
  </div>
  <div class="form-group">
    <label for="exampleInputPassword1">Confrim</label>
    <input type="password" class="form-control" id="confirm" placeholder="Confirm" name="confirm">
  </div>
  <div class="form-group">
    <label for="lblFullName">Full Name</label>
    <input type="text" class="form-control" id="fullname" placeholder="Full Name" name="fullname">
  </div>
  <div class="form-group">
    <label for="lblFullName">Address</label>
    <input type="text" class="form-control" id="address" placeholder="Address" name="address">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
</form>
